  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/noahpeters/ftops/issues"
  },
  "homepage": "https://github.com/noahpeters/ftops#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260116.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.0",
    "wrangler": "^4.59.2"
  }
}
