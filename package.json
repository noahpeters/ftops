{
  "name": "ftops",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier \"**/*.{ts,js,json,md}\" --write",
    "format:check": "prettier \"**/*.{ts,js,json,md}\" --check",
    "cf:login": "wrangler login",
    "cf:deploy": "wrangler deploy",
    "db:execute": "wrangler d1 execute ftops-db --command",
    "db:execute:remote": "wrangler d1 execute --remote ftops-db --command",
    "db:migrations:create": "wrangler d1 migrations create ftops-db",
    "db:migrations:list:local": "wrangler d1 migrations list ftops-db --local",
    "db:migrations:list:remote": "wrangler d1 migrations list ftops-db --remote",
    "db:migrate:local": "wrangler d1 migrations apply ftops-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply ftops-db --remote"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahpeters/ftops.git"
  },
  "keywords": [],
