{
  "name": "ftops-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "test": "npm run typecheck",
    "typecheck": "tsc --noEmit",
    "preview": "react-router preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "tidy": "npm run format:check && npm run lint && npm run typecheck",
    "verify": "npm run tidy",
    "deploy": "npm run verify && react-router build && wrangler deploy",
    "ci": "npm run verify"
  },
  "dependencies": {
    "@react-router/cloudflare": "^7.0.0",
    "@stylexjs/stylex": "^0.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.0",
    "react-router-dom": "^7.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.0.0",
    "@stylexjs/unplugin": "^0.17.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "typescript": "5.5.4",
    "vite": "^5.4.0"
  }
}
